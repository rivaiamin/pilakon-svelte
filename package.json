{
	"name": "kit-template-default",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"lint": "prettier --check .",
		"format": "prettier --write .",
		"docker:build": "docker build -t pilakon-svelte .",
		"docker:build:prod": "docker build --build-arg PUBLIC_SUPABASE_URL=$PUBLIC_SUPABASE_URL --build-arg PUBLIC_SUPABASE_ANON_KEY=$PUBLIC_SUPABASE_ANON_KEY -t pilakon-svelte .",
		"docker:run": "docker run -p 3000:3000 pilakon-svelte",
		"docker:dev": "docker compose --profile dev up app-dev",
		"docker:prod": "docker compose up app",
		"docker:nginx": "docker compose up nginx app",
		"docker:nginx:build": "docker compose build nginx app",
		"docker:down": "docker compose down"
	},
	"devDependencies": {
		"@fontsource/fira-mono": "^5.2.6",
		"@neoconfetti/svelte": "^2.2.2",
		"@sveltejs/adapter-node": "^3.0.3",
		"@sveltejs/kit": "^2.37.1",
		"@sveltejs/vite-plugin-svelte": "^3.1.2",
		"autoprefixer": "^10.4.21",
		"postcss": "^8.5.6",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"svelte": "^4.2.20",
		"svelte-check": "^3.8.6",
		"tailwindcss": "^3.4.17",
		"typescript": "^5.9.2",
		"vite": "^5.4.19"
	},
	"type": "module",
	"dependencies": {
		"@supabase/auth-helpers-sveltekit": "^0.12.0",
		"@supabase/supabase-js": "^2.57.2",
		"lucide-svelte": "^0.542.0",
		"svelte-dnd-action": "^0.9.65"
	}
}
